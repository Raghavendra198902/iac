{
  "dashboard": {
    "title": "Enterprise Architecture Responsibilities",
    "uid": "ea-responsibilities",
    "tags": ["ea", "architecture", "responsibilities"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "1m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "EA Health Score",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "ea_health_score",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            },
            "max": 100,
            "min": 0
          }
        }
      },
      {
        "id": 2,
        "title": "Active Initiatives",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 0},
        "targets": [
          {
            "expr": "ea_active_initiatives_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 3,
        "title": "Stakeholder Engagement",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "ea_stakeholder_engagement_count",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area"
        }
      },
      {
        "id": 4,
        "title": "Total Value Delivered",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "ea_total_value_delivered",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 1
          }
        }
      },
      {
        "id": 5,
        "title": "Compliance Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 20, "y": 0},
        "targets": [
          {
            "expr": "ea_compliance_rate",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 85, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Completion by Responsibility Area",
        "type": "bar",
        "gridPos": {"h": 10, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "ea_responsibility_completion_percent",
            "legendFormat": "{{area}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": null, "color": "red"},
                {"value": 60, "color": "yellow"},
                {"value": 80, "color": "green"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 7,
        "title": "Monthly Initiatives Trend",
        "type": "graph",
        "gridPos": {"h": 10, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "rate(ea_initiatives_created_total[30d])",
            "legendFormat": "New Initiatives",
            "refId": "A"
          },
          {
            "expr": "rate(ea_initiatives_completed_total[30d])",
            "legendFormat": "Completed Initiatives",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 8,
        "title": "Documentation Coverage",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 18},
        "targets": [
          {
            "expr": "ea_documentation_coverage_by_type",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "displayLabels": ["name", "percent"]
        }
      },
      {
        "id": 9,
        "title": "Maturity Assessment",
        "type": "radar",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 18},
        "targets": [
          {
            "expr": "ea_maturity_score_by_dimension",
            "legendFormat": "{{dimension}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "max": 5,
            "min": 0
          }
        }
      },
      {
        "id": 10,
        "title": "Risk Distribution",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 18},
        "targets": [
          {
            "expr": "ea_risks_by_severity",
            "legendFormat": "{{severity}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {"type": "value", "options": {"low": {"color": "green"}}},
              {"type": "value", "options": {"medium": {"color": "yellow"}}},
              {"type": "value", "options": {"high": {"color": "red"}}}
            ]
          }
        }
      },
      {
        "id": 11,
        "title": "Recent Activities",
        "type": "table",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 26},
        "targets": [
          {
            "expr": "topk(10, ea_activity_log)",
            "refId": "A",
            "format": "table"
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "timestamp"},
              "properties": [{"id": "unit", "value": "dateTimeAsIso"}]
            }
          ]
        }
      },
      {
        "id": 12,
        "title": "KPI Achievement Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "avg(ea_kpi_achievement_rate) by (kpi_name)",
            "legendFormat": "{{kpi_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0
          }
        }
      },
      {
        "id": 13,
        "title": "Cost vs Budget",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 34},
        "targets": [
          {
            "expr": "ea_actual_cost_by_area",
            "legendFormat": "Actual: {{area}}",
            "refId": "A"
          },
          {
            "expr": "ea_budget_by_area",
            "legendFormat": "Budget: {{area}}",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD"
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "bottom"}
        }
      }
    ]
  }
}
